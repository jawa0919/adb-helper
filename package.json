{
  "name": "adb-helper",
  "displayName": "ADB Helper",
  "description": "Devices Wifi Connect, File Explorer, APK Manager",
  "version": "0.1.1",
  "publisher": "jawa0919",
  "author": {
    "name": "jawa0919",
    "email": "jawa0919@163.com"
  },
  "homepage": "https://github.com/jawa0919/adb-helper",
  "bugs": {
    "url": "https://github.com/jawa0919/adb-helper/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jawa0919/adb-helper.git"
  },
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.57.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/AndroidManifest.xml",
    "workspaceContains:**.apk"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "adb-helper.Adb.Start",
        "title": "ADB Helper Start"
      },
      {
        "command": "adb-helper.Devices.Refresh",
        "title": "Refresh Devices",
        "icon": "$(refresh)"
      },
      {
        "command": "adb-helper.Devices.Disconnect",
        "title": "Disconnect all wifi connect Devices",
        "icon": "$(debug-disconnect)"
      },
      {
        "command": "adb-helper.Devices.Connect",
        "title": "Connect wifi Device",
        "icon": "$(radio-tower)"
      },
      {
        "command": "adb-helper.Devices.OpenSDCardExplorer",
        "title": "Open SDCard Explorer"
      },
      {
        "command": "adb-helper.Devices.OpenRootExplorer",
        "title": "Open Root Explorer"
      },
      {
        "command": "adb-helper.Devices.OpenAppManager",
        "title": "Open App Manager"
      },
      {
        "command": "adb-helper.Devices.Screenshot",
        "title": "Screenshot"
      },
      {
        "command": "adb-helper.Explorer.Refresh",
        "title": "Refresh Explorer",
        "icon": "$(refresh)"
      },
      {
        "command": "adb-helper.Explorer.NewDirectory",
        "title": "New Directory"
      },
      {
        "command": "adb-helper.Explorer.SaveAs",
        "title": "Save As..."
      },
      {
        "command": "adb-helper.Explorer.UploadFile",
        "title": "Upload File"
      },
      {
        "command": "adb-helper.Explorer.UploadDirectory",
        "title": "Upload Directory"
      },
      {
        "command": "adb-helper.Explorer.Delete",
        "title": "Delete"
      },
      {
        "command": "adb-helper.Explorer.CopyPath",
        "title": "Copy Path"
      },
      {
        "command": "adb-helper.Manager.Refresh",
        "title": "Refresh App Manager",
        "icon": "$(refresh)"
      },
      {
        "command": "adb-helper.Manager.Install",
        "title": "Install New Apk",
        "icon": "$(add)"
      },
      {
        "command": "adb-helper.Manager.Install_r_t",
        "title": "Update/Install Apk"
      },
      {
        "command": "adb-helper.Manager.Uninstall",
        "title": "Uninstall Apk"
      },
      {
        "command": "adb-helper.Manager.Uninstall_k",
        "title": "Uninstall Apk And Keep Data"
      },
      {
        "command": "adb-helper.Manager.Clear",
        "title": "Clear Apk Data"
      },
      {
        "command": "adb-helper.Manager.ExportApk",
        "title": "Export Apk"
      },
      {
        "command": "adb-helper.Command.Refresh",
        "title": "Refresh Command",
        "icon": "$(refresh)"
      },
      {
        "command": "adb-helper.Command.RunCommand",
        "title": "Run Command",
        "icon": "$(edit)"
      },
      {
        "command": "adb-helper.Command.RunHistoryCommand",
        "title": "Run History Command",
        "icon": "$(run)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "adb-helper",
          "title": "ADB Helper",
          "icon": "assets/adb-helper.svg"
        }
      ]
    },
    "views": {
      "adb-helper": [
        {
          "id": "adb-helper.Devices",
          "name": "Android Devices",
          "when": "adb-helper:isADBSupport"
        },
        {
          "id": "adb-helper.Explorer",
          "name": "Device Explorer",
          "when": "adb-helper:isADBSupport"
        },
        {
          "id": "adb-helper.Manager",
          "name": "Device App Manager",
          "when": "adb-helper:isADBSupport"
        },
        {
          "id": "adb-helper.Command",
          "name": "Adb Command History",
          "when": "adb-helper:isADBSupport"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "adb-helper.Devices.Refresh",
          "when": "view == adb-helper.Devices",
          "group": "navigation@1"
        },
        {
          "command": "adb-helper.Explorer.Refresh",
          "when": "view == adb-helper.Explorer && adb-helper:showExplorer",
          "group": "navigation@1"
        },
        {
          "command": "adb-helper.Manager.Refresh",
          "when": "view == adb-helper.Manager && adb-helper:showManager",
          "group": "navigation@2"
        },
        {
          "command": "adb-helper.Manager.Install",
          "when": "view == adb-helper.Manager && adb-helper:showManager",
          "group": "navigation@1"
        },
        {
          "command": "adb-helper.Command.Refresh",
          "when": "view == adb-helper.Command",
          "group": "navigation@2"
        },
        {
          "command": "adb-helper.Command.RunCommand",
          "when": "view == adb-helper.Command",
          "group": "navigation@1"
        }
      ],
      "view/item/context": [
        {
          "command": "adb-helper.Devices.Disconnect",
          "when": "view == adb-helper.Devices && viewItem == ipGroup",
          "group": "inline"
        },
        {
          "command": "adb-helper.Devices.Connect",
          "when": "view == adb-helper.Devices && viewItem == usb",
          "group": "inline"
        },
        {
          "command": "adb-helper.Devices.OpenSDCardExplorer",
          "when": "view == adb-helper.Devices && viewItem != ipGroup",
          "group": "d1@1"
        },
        {
          "command": "adb-helper.Devices.OpenRootExplorer",
          "when": "view == adb-helper.Devices && viewItem != ipGroup",
          "group": "d1@2"
        },
        {
          "command": "adb-helper.Devices.OpenAppManager",
          "when": "view == adb-helper.Devices && viewItem != ipGroup",
          "group": "d2@1"
        },
        {
          "command": "adb-helper.Devices.Screenshot",
          "when": "view == adb-helper.Devices && viewItem != ipGroup",
          "group": "d3@1"
        },
        {
          "command": "adb-helper.Explorer.NewDirectory",
          "when": "view ==adb-helper.Explorer",
          "group": "e1@1"
        },
        {
          "command": "adb-helper.Explorer.SaveAs",
          "when": "view ==adb-helper.Explorer",
          "group": "e2@1"
        },
        {
          "command": "adb-helper.Explorer.UploadFile",
          "when": "view == adb-helper.Explorer && viewItem == directory",
          "group": "e3@2"
        },
        {
          "command": "adb-helper.Explorer.UploadDirectory",
          "when": "view == adb-helper.Explorer && viewItem == directory",
          "group": "e3@3"
        },
        {
          "command": "adb-helper.Explorer.Delete",
          "when": "view ==adb-helper.Explorer",
          "group": "e3@4"
        },
        {
          "command": "adb-helper.Explorer.CopyPath",
          "when": "view ==adb-helper.Explorer"
        },
        {
          "command": "adb-helper.Manager.Install_r_t",
          "when": "view == adb-helper.Manager",
          "group": "m1@1"
        },
        {
          "command": "adb-helper.Manager.Uninstall",
          "when": "view == adb-helper.Manager",
          "group": "m2@1"
        },
        {
          "command": "adb-helper.Manager.Uninstall_k",
          "when": "view == adb-helper.Manager",
          "group": "m2@2"
        },
        {
          "command": "adb-helper.Manager.Clear",
          "when": "view == adb-helper.Manager",
          "group": "m3@1"
        },
        {
          "command": "adb-helper.Manager.ExportApk",
          "when": "view == adb-helper.Manager",
          "group": "m4@1"
        },
        {
          "command": "adb-helper.Command.RunHistoryCommand",
          "when": "view == adb-helper.Command",
          "group": "inline"
        }
      ]
    },
    "configuration": [
      {
        "title": "ADB Helper",
        "properties": {
          "adb-helper.startPort": {
            "type": "number",
            "default": "5555",
            "description": "wifi Connect Start Port",
            "scope": "window"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "execa": "^5.1.1"
  }
}
