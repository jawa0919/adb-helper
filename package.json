{
  "name": "adb-helper",
  "displayName": "ADB Helper",
  "description": "Devices Wifi Connect, File Explorer, APK Manager",
  "version": "0.2.9",
  "publisher": "jawa0919",
  "author": {
    "name": "jawa0919",
    "email": "jawa0919@163.com"
  },
  "homepage": "https://github.com/jawa0919/adb-helper",
  "bugs": {
    "url": "https://github.com/jawa0919/adb-helper/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jawa0919/adb-helper.git"
  },
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.57.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/AndroidManifest.xml",
    "workspaceContains:**.apk"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "adb-helper.ADBHelper.Start",
        "title": "ADB Helper Start"
      },
      {
        "command": "adb-helper.Manager.Refresh",
        "title": "Refresh Manager",
        "icon": "$(refresh)"
      },
      {
        "command": "adb-helper.Manager.Device.Swap",
        "title": "Swap current device",
        "icon": "$(device-mobile)"
      },
      {
        "command": "adb-helper.Manager.Wifi.History",
        "title": "Wifi Device History"
      },
      {
        "command": "adb-helper.Manager.Wifi.Disconnect",
        "title": "Disconnect All Wifi Connect Devices"
      },
      {
        "command": "adb-helper.Manager.Wifi.InputIPConnect",
        "title": "Input Device IP Connect"
      },
      {
        "command": "adb-helper.Manager.Device.Screenshot",
        "title": "Device Screenshot",
        "icon": "$(device-camera)"
      },
      {
        "command": "adb-helper.Manager.Device.Install",
        "title": "Install New Apk",
        "icon": "$(diff-added)"
      },
      {
        "command": "adb-helper.Manager.Device.ConnectWifi",
        "title": "Connect Wifi",
        "icon": "$(broadcast)"
      },
      {
        "command": "adb-helper.Manager.Apk.Install_r_t",
        "title": "Update/Install Apk",
        "icon": "$(log-in)"
      },
      {
        "command": "adb-helper.Manager.Apk.Uninstall",
        "title": "Uninstall Apk",
        "icon": "$(log-out)"
      },
      {
        "command": "adb-helper.Manager.Apk.Wipe",
        "title": "Wipe Apk Data"
      },
      {
        "command": "adb-helper.Manager.Apk.Export",
        "title": "Export Apk File"
      },
      {
        "command": "adb-helper.Explorer.Refresh",
        "title": "Refresh Explorer",
        "icon": "$(refresh)"
      },
      {
        "command": "adb-helper.Explorer.RootPath",
        "title": "(TEST) Root Path"
      },
      {
        "command": "adb-helper.Explorer.NewDirectory",
        "title": "New Directory"
      },
      {
        "command": "adb-helper.Explorer.SaveAs",
        "title": "Save As..."
      },
      {
        "command": "adb-helper.Explorer.UploadFile",
        "title": "Upload File"
      },
      {
        "command": "adb-helper.Explorer.UploadDirectory",
        "title": "Upload Directory"
      },
      {
        "command": "adb-helper.Explorer.Delete",
        "title": "Delete"
      },
      {
        "command": "adb-helper.Explorer.CopyPath",
        "title": "Copy Path"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "adb-helper",
          "title": "ADB Helper",
          "icon": "assets/adb-helper.svg"
        }
      ]
    },
    "views": {
      "adb-helper": [
        {
          "id": "adb-helper.Manager",
          "name": "Manager",
          "when": "adb-helper:isADBSupport"
        },
        {
          "id": "adb-helper.Explorer",
          "name": "Explorer",
          "when": "adb-helper:isADBSupport"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "adb-helper.Manager.Refresh",
          "when": "view == adb-helper.Manager",
          "group": "navigation@1"
        },
        {
          "command": "adb-helper.Manager.Device.Swap",
          "when": "view == adb-helper.Manager",
          "group": "navigation@2"
        },
        {
          "command": "adb-helper.Manager.Wifi.History",
          "when": "view == adb-helper.Manager",
          "group": "w1@1"
        },
        {
          "command": "adb-helper.Manager.Wifi.Disconnect",
          "when": "view == adb-helper.Manager",
          "group": "w1@2"
        },
        {
          "command": "adb-helper.Manager.Wifi.InputIPConnect",
          "when": "view == adb-helper.Manager",
          "group": "w1@3"
        },
        {
          "command": "adb-helper.Explorer.Refresh",
          "when": "view == adb-helper.Explorer",
          "group": "navigation@1"
        },
        {
          "command": "adb-helper.Explorer.RootPath",
          "when": "view == adb-helper.Explorer",
          "group": "e1@1"
        }
      ],
      "view/item/context": [
        {
          "command": "adb-helper.Manager.Device.Screenshot",
          "when": "view == adb-helper.Manager && viewItem == currentDevice",
          "group": "inline"
        },
        {
          "command": "adb-helper.Manager.Device.Install",
          "when": "view == adb-helper.Manager && viewItem == currentDevice",
          "group": "inline"
        },
        {
          "command": "adb-helper.Manager.Device.ConnectWifi",
          "when": "view == adb-helper.Manager && viewItem == currentDevice",
          "group": "inline"
        },
        {
          "command": "adb-helper.Manager.Apk.Install_r_t",
          "when": "view == adb-helper.Manager && viewItem != currentDevice",
          "group": "d1@1"
        },
        {
          "command": "adb-helper.Manager.Apk.Uninstall",
          "when": "view == adb-helper.Manager && viewItem != currentDevice",
          "group": "d2@1"
        },
        {
          "command": "adb-helper.Manager.Apk.Wipe",
          "when": "view == adb-helper.Manager && viewItem != currentDevice",
          "group": "d3@1"
        },
        {
          "command": "adb-helper.Manager.Apk.Export",
          "when": "view == adb-helper.Manager && viewItem != currentDevice",
          "group": "d4@1"
        },
        {
          "command": "adb-helper.Explorer.NewDirectory",
          "when": "view == adb-helper.Explorer",
          "group": "e1@1"
        },
        {
          "command": "adb-helper.Explorer.SaveAs",
          "when": "view == adb-helper.Explorer",
          "group": "e2@1"
        },
        {
          "command": "adb-helper.Explorer.UploadFile",
          "when": "view == adb-helper.Explorer && viewItem == directory",
          "group": "e3@2"
        },
        {
          "command": "adb-helper.Explorer.UploadDirectory",
          "when": "view == adb-helper.Explorer && viewItem == directory",
          "group": "e3@3"
        },
        {
          "command": "adb-helper.Explorer.Delete",
          "when": "view == adb-helper.Explorer",
          "group": "e3@4"
        },
        {
          "command": "adb-helper.Explorer.CopyPath",
          "when": "view == adb-helper.Explorer",
          "group": "e4@1"
        }
      ]
    },
    "configuration": [
      {
        "title": "ADB Helper",
        "properties": {
          "adb-helper.startPort": {
            "type": "number",
            "default": "5555",
            "description": "wifi Connect Start Port",
            "scope": "window"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "execa": "^5.1.1"
  }
}
