{
	"name": "adb-helper",
	"displayName": "ADB Helper",
	"description": "ADB Helper",
	"version": "0.4.0",
	"icon": "assets/icon.png",
	"publisher": "jawa0919",
	"author": {
		"name": "jawa0919",
		"email": "jawa0919@163.com"
	},
	"homepage": "https://github.com/jawa0919/adb-helper",
	"bugs": {
		"url": "https://github.com/jawa0919/adb-helper/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/jawa0919/adb-helper.git"
	},
	"engines": {
		"vscode": "^1.57.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:AndroidManifest.xml",
		"workspaceContains:*/AndroidManifest.xml",
		"workspaceContains:*/*/AndroidManifest.xml",
		"workspaceContains:*.apk",
		"workspaceContains:*/*.apk",
		"workspaceContains:*/*/*.apk",
		"workspaceContains:.metadata",
		"workspaceContains:*/.metadata",
		"workspaceContains:*/*/.metadata",
		"onView:adb-helper.DeviceManager",
		"onView:adb-helper.ExplorerManager",
		"onCommand:adb-helper.restartAdb"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "adb-helper.restartAdb",
				"title": "Start/Restart Adb",
				"icon": "$(sync)"
			},
			{
				"command": "adb-helper.refreshDeviceManager",
				"title": "Refresh Device Manager",
				"icon": "$(refresh)"
			},
			{
				"command": "adb-helper.ipConnect",
				"title": "Input Device IP Connect",
				"icon": "$(rss)"
			},
			{
				"command": "adb-helper.ipConnectHistory",
				"title": "IP Connect History",
				"icon": "$(history)"
			}
		],
		"configuration": [
			{
				"title": "ADB Helper Custom",
				"properties": {
					"adb-helper.adbBinPath": {
						"order": 0,
						"description": "Custom ADB Bin Path",
						"type": "string",
						"default": "",
						"scope": "machine-overridable"
					},
					"adb-helper.androidSdkPath": {
						"order": 1,
						"type": "string",
						"description": "Custom Android SDK Path",
						"default": "",
						"scope": "machine-overridable"
					},
					"adb-helper.flutterSdkPath": {
						"order": 2,
						"type": "string",
						"description": "Custom Flutter SDK Path",
						"default": "",
						"scope": "machine-overridable"
					},
					"adb-helper.explorerRootPathList": {
						"order": 3,
						"type": "array",
						"description": "Custom Explorer RootPath List",
						"items": {
							"type": "string"
						},
						"default": [
							"/sdcard/",
							"/",
							"/sdcard/Android/data/",
							"/sdcard/DCIM/",
							"/sdcard/Download/"
						],
						"scope": "machine-overridable"
					}
				}
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "adb-helper",
					"title": "ADB Helper",
					"icon": "assets/adb-helper.svg"
				}
			]
		},
		"views": {
			"adb-helper": [
				{
					"id": "adb-helper.DeviceManager",
					"name": "Devices Manager",
					"contextualTitle": "Device Apk Manager"
				},
				{
					"id": "adb-helper.ExplorerManager",
					"name": "Explorer Manager",
					"contextualTitle": "Device Explorer Manager"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "adb-helper.DeviceManager",
				"contents": "Android Device Not Found. \nPlease Try\n[Refresh Device Manager](command:adb-helper.refreshDeviceManager)\nOr\n[Restart ADB Helper](command:adb-helper.restartAdb)\nAnd You Can Use IP Connect\n[Input Device IP Address](command:adb-helper.ipConnect)\n"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "adb-helper.restartAdb"
				}
			],
			"view/title": [
				{
					"command": "adb-helper.restartAdb",
					"when": "view == adb-helper.DeviceManager",
					"group": "navigation@995"
				},
				{
					"command": "adb-helper.refreshDeviceManager",
					"when": "view == adb-helper.DeviceManager",
					"group": "navigation@996"
				},
				{
					"command": "adb-helper.ipConnect",
					"when": "view == adb-helper.DeviceManager",
					"group": "999_wifi@995"
				},
				{
					"command": "adb-helper.ipConnectHistory",
					"when": "view == adb-helper.DeviceManager",
					"group": "999_wifi@996"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"@types/vscode": "^1.57.0",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"@vscode/test-electron": "^2.1.3",
		"eslint": "^8.14.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"typescript": "^4.6.4"
	},
	"dependencies": {
		"execa": "^5.1.1"
	}
}