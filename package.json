{
  "name": "adb-helper",
  "displayName": "ADB Helper",
  "description": "Devices Wifi Connect, File Explorer, APK Manager",
  "version": "0.3.3",
  "publisher": "jawa0919",
  "author": {
    "name": "jawa0919",
    "email": "jawa0919@163.com"
  },
  "homepage": "https://github.com/jawa0919/adb-helper",
  "bugs": {
    "url": "https://github.com/jawa0919/adb-helper/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jawa0919/adb-helper.git"
  },
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.57.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/AndroidManifest.xml",
    "workspaceContains:**.apk"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "adb-helper.ADBHelper.Start",
        "title": "ADB Helper Start"
      },
      {
        "command": "adb-helper.Device.Refresh",
        "title": "Refresh Manager",
        "icon": "$(refresh)"
      },
      {
        "command": "adb-helper.Device.Swap",
        "title": "Swap Current Device",
        "icon": "$(device-mobile)"
      },
      {
        "command": "adb-helper.Device.Install_r_t",
        "title": "Install To Phone"
      },
      {
        "command": "adb-helper.Device.DisconnectWifi",
        "title": "Disconnect Wifi Connect",
        "icon": "$(debug-disconnect)"
      },
      {
        "command": "adb-helper.Device.WifiHistory",
        "title": "(TEST) Wifi Device History"
      },
      {
        "command": "adb-helper.Device.InputIPConnect",
        "title": "(TEST) Input Device IP Connect"
      },
      {
        "command": "adb-helper.Device.ConnectWifi",
        "title": "Connect Wifi",
        "icon": "$(broadcast)"
      },
      {
        "command": "adb-helper.Device.Screenshot",
        "title": "Device Screenshot",
        "icon": "$(device-camera)"
      },
      {
        "command": "adb-helper.Manager.Refresh",
        "title": "Refresh Manager",
        "icon": "$(refresh)"
      },
      {
        "command": "adb-helper.Manager.Install",
        "title": "Install New Apk",
        "icon": "$(diff-added)"
      },
      {
        "command": "adb-helper.Manager.Install_r_t",
        "title": "Update/Install Apk",
        "icon": "$(log-in)"
      },
      {
        "command": "adb-helper.Manager.Uninstall",
        "title": "Uninstall Apk",
        "icon": "$(log-out)"
      },
      {
        "command": "adb-helper.Manager.Wipe",
        "title": "Wipe Apk Data"
      },
      {
        "command": "adb-helper.Manager.Export",
        "title": "Export Apk File"
      },
      {
        "command": "adb-helper.Explorer.Refresh",
        "title": "Refresh Explorer",
        "icon": "$(refresh)"
      },
      {
        "command": "adb-helper.Explorer.SwapRootPath",
        "title": "(TEST) Swap Root Path"
      },
      {
        "command": "adb-helper.Explorer.NewDirectory",
        "title": "New Directory"
      },
      {
        "command": "adb-helper.Explorer.SaveAs",
        "title": "Save As..."
      },
      {
        "command": "adb-helper.Explorer.UploadFile",
        "title": "Upload File"
      },
      {
        "command": "adb-helper.Explorer.UploadDirectory",
        "title": "Upload Directory"
      },
      {
        "command": "adb-helper.Explorer.Delete",
        "title": "Delete"
      },
      {
        "command": "adb-helper.Explorer.CopyPath",
        "title": "Copy Path"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "adb-helper",
          "title": "ADB Helper",
          "icon": "assets/adb-helper.svg"
        }
      ]
    },
    "views": {
      "adb-helper": [
        {
          "id": "adb-helper.Manager",
          "name": "Manager",
          "when": "adb-helper:isADBSupport"
        },
        {
          "id": "adb-helper.Explorer",
          "name": "Explorer",
          "when": "adb-helper:isADBSupport"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "when": "view == adb-helper.Manager",
          "command": "adb-helper.Device.Refresh",
          "group": "navigation@1"
        },
        {
          "when": "view == adb-helper.Manager",
          "command": "adb-helper.Device.Swap",
          "group": "navigation@2"
        },
        {
          "when": "view == adb-helper.Manager",
          "command": "adb-helper.Device.DisconnectWifi",
          "group": "navigation@3"
        },
        {
          "when": "view == adb-helper.Manager",
          "command": "adb-helper.Device.WifiHistory",
          "group": "w1@1"
        },
        {
          "when": "view == adb-helper.Manager",
          "command": "adb-helper.Device.InputIPConnect",
          "group": "w1@2"
        },
        {
          "when": "view == adb-helper.Explorer",
          "command": "adb-helper.Explorer.Refresh",
          "group": "navigation@1"
        },
        {
          "when": "view == adb-helper.Explorer",
          "command": "adb-helper.Explorer.SwapRootPath",
          "group": "e1@1"
        }
      ],
      "view/item/context": [
        {
          "when": "view == adb-helper.Manager && viewItem == currentDevice",
          "command": "adb-helper.Device.ConnectWifi",
          "group": "inline@1"
        },
        {
          "when": "view == adb-helper.Manager && viewItem == currentDevice",
          "command": "adb-helper.Device.Screenshot",
          "group": "inline@2"
        },
        {
          "when": "view == adb-helper.Manager && viewItem == currentDevice",
          "command": "adb-helper.Manager.Install",
          "group": "inline@3"
        },
        {
          "when": "view == adb-helper.Manager && viewItem == apk",
          "command": "adb-helper.Manager.Install_r_t",
          "group": "d1@1"
        },
        {
          "when": "view == adb-helper.Manager && viewItem == apk",
          "command": "adb-helper.Manager.Uninstall",
          "group": "d2@1"
        },
        {
          "when": "view == adb-helper.Manager && viewItem == apk",
          "command": "adb-helper.Manager.Wipe",
          "group": "d3@1"
        },
        {
          "when": "view == adb-helper.Manager && viewItem == apk",
          "command": "adb-helper.Manager.Export",
          "group": "d4@1"
        },
        {
          "when": "view == adb-helper.Explorer",
          "command": "adb-helper.Explorer.NewDirectory",
          "group": "e1@1"
        },
        {
          "when": "view == adb-helper.Explorer",
          "command": "adb-helper.Explorer.SaveAs",
          "group": "e2@1"
        },
        {
          "when": "view == adb-helper.Explorer && viewItem == directory",
          "command": "adb-helper.Explorer.UploadFile",
          "group": "e3@1"
        },
        {
          "when": "view == adb-helper.Explorer && viewItem == directory",
          "command": "adb-helper.Explorer.UploadDirectory",
          "group": "e3@2"
        },
        {
          "when": "view == adb-helper.Explorer",
          "command": "adb-helper.Explorer.Delete",
          "group": "e3@3"
        },
        {
          "when": "view == adb-helper.Explorer",
          "command": "adb-helper.Explorer.CopyPath",
          "group": "e4@1"
        }
      ],
      "explorer/context": [
        {
          "when": "resourceExtname == .apk",
          "command": "adb-helper.Device.Install_r_t",
          "group": "apk"
        }
      ]
    },
    "configuration": [
      {
        "title": "ADB Helper",
        "properties": {
          "adb-helper.explorerRootPathList": {
            "default": [
              "/",
              "/sdcard/Download/",
              "/sdcard/DCIM/",
              "/sdcard/Music/",
              "/sdcard/Android/data/"
            ],
            "items": {
              "type": "string"
            },
            "markdownDescription": "Explorer RootPath List",
            "type": "array"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "execa": "^5.1.1"
  }
}
